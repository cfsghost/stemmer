{
	"platform": "cubieboard",
	"repo": {
		"General": {
			"arch": "armhf",
			"cleanup": true,
			"noauth": true,
			"unpack": true,
			"debootstrap": [ "Grip", "Debian" ],
			"aptsources": [ "Grip", "Debian" ]
		},
		"Grip": {
			"packages": [
				"libudev0",
				"ntpdate",
				"net-tools",
				"isc-dhcp-client",
				"ifupdown",
				"parted",
				"policykit-1",
				"dbus",
				"dbus-x11",
				"udisks2",
				"connman",
				"libgl1-mesa-dri"
			],
			"source": "http://www.emdebian.org/grip",
			"keyring": "emdebian-archive-keyring",
			"suite": "sid-grip"
		},
		"Debian": {
			"packages": [ "fonts-droid" ],
			"source": "http://ftp.us.debian.org/debian",
			"keyring": "debian-archive-keyring",
			"suite": "sid"
		}
	}
}
