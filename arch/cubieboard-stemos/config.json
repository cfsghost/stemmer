{
	"platform": "cubieboard",
	"repo": {
		"General": {
			"arch": "armhf",
			"cleanup": true,
			"noauth": true,
			"unpack": true,
			"debootstrap": [ "Grip", "Debian" ],
			"aptsources": [ "Grip", "Debian" ]
		},
		"Grip": {
			"packages": [
				"ntpdate",
				"net-tools",
				"isc-dhcp-client",
				"ifupdown",
				"parted",
				"policykit-1",
				"dbus",
				"dbus-x11",
				"udisks2",
				"connman",
				"alsa-base"
			],
			"source": "http://www.emdebian.org/grip",
			"keyring": "emdebian-archive-keyring",
			"suite": "sid-grip"
		},
		"Debian": {
			"packages": [
				"fonts-droid",
				"xserver-xorg",
				"xserver-xorg-input-all",
				"xserver-xorg-video-fbdev",
				"xserver-xorg-core",
				"xinit",
				"xterm",
				"libgl1-mesa-dri",
				"libqt5core5a",
				"libqt5quick5",
				"libqt5qml5",
				"libqt5gui5",
				"libqt5network5",
				"libqt5dbus5",
				"libqt5multimedia5",
				"libqt5multimedia5-plugins",
				"qtdeclarative5-qtquick2-plugin",
				"qtdeclarative5-qtmultimedia-plugin",
				"qtdeclarative5-qtsensors-plugin"
			],
			"source": "http://ftp.us.debian.org/debian",
			"keyring": "debian-archive-keyring",
			"suite": "sid"
		},
		"Multimedia": {
			"source": "http://www.deb-multimedia.org",
			"keyring": "deb-multimedia-keyring",
			"components": "main non-free",
			"suite": "sid"
		}
	}
}
